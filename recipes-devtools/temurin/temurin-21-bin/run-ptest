#!/bin/sh
set -euxo pipefail

FILE=/usr/lib/jvm/java-21-temurin/bin/java
if [ -f "$FILE" ]; then
    echo "$FILE exists. Using alternate java name to test."
    $FILE --version
else
    echo "$FILE does not exist. Using default java to test."
    java --version
fi

RETVAL=$?
if [ $RETVAL -eq 0 ] ; then
    echo "PASS: temurin-21-bin startup test"
else
    echo "FAIL: temurin-21-bin startup test"
fi